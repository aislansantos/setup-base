{
  "name": "setup-base",
  "version": "1.0.0",
  "description": "Setup b√°sico para usar em projetos, typescript, eslint, jest eHusky &amp; Lint Staged (commit hooks)",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:dev": "nodemon --watch \"src\" --exec \"ts-node -r tsconfig-paths/register src/server.ts\" -e ts",
    "husk:prepare": "husky init",
    "prepare": "husky",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test --watch",
    "test:staged": "npm test --findRelatedTests",
    "test:push": "npm test --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.6",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "sequelize": "^6.37.2",
    "validator": "^13.11.0"
  }
}
